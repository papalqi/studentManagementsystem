package com.studentgrade.backinfo;

import java.math.BigDecimal;
import java.util.ArrayList;
import java.util.List;

/**
 * <p>Title: MajorListInfo<／p>
 * <p>Description: <／p>
 * <p>Company: Oracle Group4<／p> 
 * @author XuChongtian
 * @date 2018年3月13日
 */
public class MajorListInfo {
	private int status;
	private List<MajorName> info = new ArrayList<MajorName>();
	
	
	public int getStatus() {
		return status;
	}


	public void setStatus(int status) {
		this.status = status;
	}


	public List<MajorName> getInfo() {
		return info;
	}


	public void setInfo(List<MajorName> info) {
		this.info = info;
	}


	public static class MajorName{
		private String majorname;
		private BigDecimal majorid;
		public String getMajorname() {
			return majorname;
		}
		public void setMajorname(String majorname) {
			this.majorname = majorname;
		}
		public BigDecimal getMajorid() {
			return majorid;
		}
		public void setMajorid(BigDecimal majorid) {
			this.majorid = majorid;
		}
		
		
	}
}
