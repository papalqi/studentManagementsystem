package com.studentgrade.postinfo;

import java.math.BigDecimal;

/**
 * <p>Title: SetStudentGrade<／p>
 * <p>Description: <／p>
 * <p>Company: Oracle Group4<／p> 
 * @author XuChongtian
 * @date 2018年3月17日
 */
public class SetStudentGrade {

	private BigDecimal studentid;
	private BigDecimal courseID;
	private BigDecimal score1;
	private BigDecimal score2;
	
	public BigDecimal getStudentid() {
		return studentid;
	}
	public void setStudentid(BigDecimal studentid) {
		this.studentid = studentid;
	}
	public BigDecimal getCourseID() {
		return courseID;
	}
	public void setCourseID(BigDecimal courseID) {
		this.courseID = courseID;
	}
	public BigDecimal getScore1() {
		return score1;
	}
	public void setScore1(BigDecimal score1) {
		this.score1 = score1;
	}
	public BigDecimal getScore2() {
		return score2;
	}
	public void setScore2(BigDecimal score2) {
		this.score2 = score2;
	}
	
}
